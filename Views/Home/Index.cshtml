@model List<User>
@{
    ViewData["Title"] = "Home Page";
}






<h1>All Users :</h1>
<table class="table table-striped table-dark">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Username</th>
      <th scope="col">Password</th>
      <th scope="col">JoinDate</th>
    </tr>
  </thead>
  <tbody>
    @foreach(var user in Model){
    <tr>
      <th>@user.Id</th>
      <td>@user.Username</td>
      <td>@user.Password</td>
      <td>@user.Joindate</td>
    </tr>
    }
  </tbody>
</table>

