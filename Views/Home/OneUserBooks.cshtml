@model List<BookShelfAndUser>
@{
    ViewData["Title"] = "Home Page";
}

<a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="AllBooks">All Books</a>




<h1>All Users :</h1>
<table class="table table-striped table-dark">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Username</th>
      <th scope="col">Password</th>
      <th scope="col">JoinDate</th>
    </tr>
  </thead>
  <tbody>
    @foreach(var user in Model){
    <tr>
      <th>@user.Id</th>
      <td>@user.</td>
      
    </tr>
    }
  </tbody>
</table>

