@using mvc.Models.MyClasse
@model List<BestBookViewModel>
@{
    ViewData["Title"] = "Home Page";
}

<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th>Title</th>
            <th>Count</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var book in Model)
        {
            <tr>
                <td>@book.Title</td>
                <td>@book.Count</td>
            </tr>
        }
    </tbody>
</table>
